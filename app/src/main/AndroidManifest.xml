<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zentcode.cursapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_c"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity
            android:name=".Listab"
            android:label="@string/title_activity_listab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Detalles"
            android:label="@string/title_activity_detalles"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".FavoritosActivity"/>
    </application>

</manifest>

